\begin_layout Standard
General solution:
\begin_inset Formula 
\begin{align*}
c_{t}^{H} & =w_{t}+n_{t}+\frac{\tau^{D}}{\lambda}\Phi y_{t}-\frac{\tau^{D}}{\lambda}(1-\alpha)(1-\Phi)w_{t}\\
 & =\frac{1}{1-\alpha}y_{t}+\frac{\tau^{D}}{\lambda}\Phi y_{t}+\left\{ 1-\frac{\tau^{D}}{\lambda}(1-\alpha)(1-\Phi)\right\} w_{t}\\
 & =\frac{1}{1-\alpha}y_{t}+\frac{\tau^{D}}{\lambda}\Phi y_{t}+\left\{ 1-\frac{\tau^{D}}{\lambda}(1-\alpha)(1-\Phi)\right\} \frac{(1-\alpha)\gamma+\varphi}{1-\alpha}y_{t}\\
 & =\frac{1+(1-\alpha)\gamma+\varphi}{1-\alpha}y_{t}-\frac{\tau^{D}}{\lambda}\left[(1-\Phi)\{(1-\alpha)\gamma+\varphi\}-\Phi\right]y_{t}\\
 & =\chi y_{t}=\frac{1}{1-\alpha}y_{t}+(\tilde{\tilde{\varphi}}+\tilde{\tilde{\gamma}})y_{t}\\
c_{t}^{S} & =\left\{ 1-\frac{\lambda(\chi-1)}{1-\lambda}\right\} y_{t}\\
w_{t} & =(\gamma+\varphi)(1+\tilde{\alpha})y_{t}=\left\{ \gamma+\frac{\varphi}{1-\alpha}\right\} y_{t}\\
d_{t} & =\Phi y_{t}-(1-\alpha)(1-\Phi)w_{t}\\
 & =\{\Phi-(1-\Phi)\{(1-\alpha)\gamma+\varphi\}\}y_{t}\\
mc_{t} & =w_{t}+y_{t}\alpha/(1-\alpha)=\left\{ \gamma+\frac{\varphi+\alpha}{1-\alpha}\right\} y_{t}\\
\pi_{t} & =\frac{\psi_{p}(1-\Phi)}{\eta_{p}}mc_{t}+\beta\mathbb{E}_{t}\left[\pi_{t+1}\right]\\
 & =\frac{\psi(1-\Phi)}{\eta}\left\{ \gamma+\frac{\varphi+\alpha}{1-\alpha}\right\} y_{t}\\
r_{t}^{N} & =\phi\pi_{t}-m_{t}\\
mu_{t}^{S} & =-(\gamma+\kappa^{S})c_{t}^{S}+\kappa(1-\alpha)(1-\Phi)n_{t}\\
r_{t} & =mu_{t}^{S}-E_{t}mu_{t+1}^{S}\\
r_{t} & =r_{t}^{N}-E_{t}\pi_{t+1}
\end{align*}

\end_inset


\end_layout

