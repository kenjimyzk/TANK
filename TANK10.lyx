\begin_layout Subsection
marginal utility
\end_layout

\begin_layout Standard
marginal utility
\begin_inset Formula 
\begin{align*}
mu_{t}^{S} & =-(\gamma+\kappa)\left\{ 1-\frac{\lambda(\chi-1)}{1-\lambda}\right\} y_{t}+\kappa(1-\Phi)y_{t}\\
 & =-\gamma\left\{ 1-\frac{\lambda(\chi-1)}{1-\lambda}\right\} y_{t}-\kappa\left\{ \Phi-\frac{\lambda(\chi-1)}{1-\lambda}\right\} y_{t}\\
 & =-(\gamma+\kappa\Phi)y_{t}+\frac{\lambda(\chi-1)}{1-\lambda}(\gamma+\kappa)y_{t}
\end{align*}

\end_inset

Note:
\begin_inset Formula 
\[
\gamma+\kappa>\gamma-\frac{\gamma\varphi}{\gamma+\varphi}=\frac{\gamma^{2}}{\gamma+\varphi}>0
\]

\end_inset


\end_layout

