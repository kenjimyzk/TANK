\begin_layout Standard
EIS is positive iff
\begin_inset Formula 
\[
\frac{\lambda(\chi-1)}{1-\lambda}(\gamma+\kappa)<\gamma+\kappa\Phi
\]

\end_inset


\begin_inset Formula 
\[
\lambda(\chi-1)<(1-\lambda)\frac{\gamma+\kappa\Phi}{\gamma+\kappa}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{(1-\alpha)\gamma+\varphi+\alpha}{1-\alpha}\lambda & <\tau^{D}\left[(1-\Phi)\{(1-\alpha)\gamma+\varphi\}-\Phi\right]+(1-\lambda)\frac{\gamma+\kappa\Phi}{\gamma+\kappa}\\
\left\{ \left(\gamma+\frac{\varphi+\alpha}{1-\alpha}\right)+\frac{\gamma+\kappa\Phi}{\gamma+\kappa}\right\} \lambda & <\tau^{D}\left[(1-\Phi)\{(1-\alpha)\gamma+\varphi\}-\Phi\right]+\frac{\gamma+\kappa\Phi}{\gamma+\kappa}\\
\lambda & <\frac{\tau^{D}\left[(1-\Phi)\{(1-\alpha)\gamma+\varphi\}-\Phi\right]+\frac{\gamma+\kappa\Phi}{\gamma+\kappa}}{\left(\gamma+\frac{\varphi+\alpha}{1-\alpha}\right)+\frac{\gamma+\kappa\Phi}{\gamma+\kappa}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
When 
\begin_inset Formula $\alpha=\Phi=0$
\end_inset

,
\begin_inset Formula 
\begin{align*}
\lambda & <\left\{ \frac{\gamma+\varphi}{\gamma+\varphi+\frac{\gamma}{\gamma+\kappa}}\right\} \tau^{D}+\frac{\frac{\gamma}{\gamma+\kappa}}{\gamma+\varphi+\frac{\gamma}{\gamma+\kappa}}\\
 & =\frac{(\gamma+\kappa)(\gamma+\varphi)}{(\gamma+\kappa)(\gamma+\varphi)+\gamma}\tau^{D}+\frac{\gamma}{(\gamma+\kappa)(\gamma+\varphi)+\gamma}\\
 & =\tau^{D}+\frac{\gamma(1-\tau^{D})}{\gamma(\gamma+\varphi)+\xi(\varphi+1)+\gamma}\\
 & =\tau^{D}+\frac{\gamma(1-\tau^{D})}{\gamma^{2}+(\xi+\gamma)(\varphi+1)}
\end{align*}

\end_inset


\begin_inset Formula $=\tau+\frac{\gamma(1-\tau)}{(1+\varphi)(\gamma+\xi)}$
\end_inset


\end_layout

