\begin_layout Standard
Substituting 
\begin_inset Formula $n_{t}^{H}=(w_{t}-\gamma c_{t}^{H})/\varphi$
\end_inset

 and 
\begin_inset Formula $d_{t}=-(1-\alpha)(1-\Phi)w_{t}+\Phi y_{t}$
\end_inset

 into 
\begin_inset Formula $c_{t}^{H}=(1-\Phi)(1-\alpha)(w_{t}+n_{t}^{H})+\frac{\tau}{\lambda}d_{t}$
\end_inset

 leads to 
\begin_inset Formula 
\begin{align*}
c_{t}^{H} & =(1-\Phi)(1-\alpha)\{w_{t}+(w_{t}-\gamma c_{t}^{H})/\varphi\}-\frac{\tau}{\lambda}(1-\alpha)(1-\Phi)w_{t}+\frac{\tau}{\lambda}\Phi y_{t}\\
 & =\frac{(1-\alpha)(1-\Phi)\{1+\varphi(1-\tau/\lambda)\}}{1+(1-\Phi)(1-\alpha)\gamma/\varphi}\frac{w_{t}}{\varphi}+\frac{\tau}{\lambda}\frac{\Phi}{1+(1-\Phi)(1-\alpha)\gamma/\varphi}y_{t}\\
 & =(1-\alpha)(1-\Phi)\frac{1+\varphi(1-\tau/\lambda)}{\varphi+(1-\Phi)(1-\alpha)\gamma}w_{t}+\frac{\tau}{\lambda}\frac{\varphi\Phi}{\varphi+(1-\Phi)(1-\alpha)\gamma}y_{t}\\
 & =(1-\alpha)(1-\Phi)\frac{1+\varphi(1-\tau/\lambda)}{\varphi+(1-\Phi)(1-\alpha)\gamma}w_{t}+\frac{\tau}{\lambda}\frac{\varphi\Phi}{\varphi+(1-\Phi)(1-\alpha)\gamma}y_{t}
\end{align*}

\end_inset

Substituting 
\begin_inset Formula $w_{t}=\gamma c_{t}+\varphi n_{t}$
\end_inset

 and 
\begin_inset Formula $y_{t}=c_{t}=(1-\alpha)n_{t}$
\end_inset

 into the above leads to
\begin_inset Formula 
\begin{align*}
c_{t}^{H} & =(1-\alpha)(1-\Phi)\frac{1+\varphi(1-\tau/\lambda)}{\varphi+(1-\Phi)(1-\alpha)\gamma}(\gamma c_{t}+\varphi n_{t})+\frac{\tau}{\lambda}\frac{\varphi\Phi}{\varphi+(1-\Phi)(1-\alpha)\gamma}y_{t}\\
 & =(1-\alpha)(1-\Phi)\frac{1+\varphi(1-\tau/\lambda)}{\varphi+(1-\Phi)(1-\alpha)\gamma}\{\gamma+\frac{\varphi}{1-\alpha}\}y_{t}+\frac{\tau}{\lambda}\frac{\varphi\Phi}{\varphi+(1-\Phi)(1-\alpha)\gamma}y_{t}\\
 & =(1-\alpha)(1-\Phi)\{1+\varphi(1-\tau/\lambda)\}\left\{ \frac{(1-\alpha)\gamma+\varphi}{\varphi+(1-\Phi)(1-\alpha)\gamma}\frac{1}{1-\alpha}\right\} y_{t}+\frac{\varphi(\tau/\lambda)\Phi}{\varphi+(1-\Phi)(1-\alpha)\gamma}y_{t}\\
 & =\{1+\varphi(1-\tau/\lambda)\}\left\{ \frac{(1-\Phi)(1-\alpha)\gamma+(1-\Phi)\varphi}{\varphi+(1-\Phi)(1-\alpha)\gamma}\right\} y_{t}+\frac{\varphi(\tau/\lambda)\Phi}{\varphi+(1-\Phi)(1-\alpha)\gamma}y_{t}\\
 & =\{1+\varphi(1-\tau/\lambda)\}\left\{ 1-\frac{\Phi\varphi}{\varphi+(1-\Phi)(1-\alpha)\gamma}\right\} y_{t}+\frac{\varphi(\tau/\lambda)\Phi}{\varphi+(1-\Phi)(1-\alpha)\gamma}y_{t}\\
 & =\{1+\varphi(1-\tau/\lambda)\}y_{t}-\Phi\varphi\frac{\{1+\varphi(1-\tau/\lambda)\}-(\tau/\lambda)}{\varphi+(1-\Phi)(1-\alpha)\gamma}y_{t}\\
 & =\{1+\varphi(1-\tau/\lambda)\}y_{t}-\Phi\varphi\frac{(1+\varphi)(1-\tau/\lambda)}{\varphi+(1-\Phi)(1-\alpha)\gamma}y_{t}\\
 & =\{1+\varphi(1-\tau/\lambda)(1-\tilde{\Phi})\}y_{t}=(1+\tilde{\varphi})y_{t}\\
\tilde{\varphi} & =\varphi(1-\tau/\lambda)(1-\tilde{\Phi})\\
\tilde{\Phi} & =\Phi\frac{\varphi+1}{\varphi+(1-\Phi)(1-\alpha)\gamma}
\end{align*}

\end_inset


\begin_inset Formula 
\[
c_{t}^{S}=\frac{c_{t}-\lambda c_{t}^{H}}{1-\lambda}=\left\{ 1-\frac{\lambda\tilde{\varphi}}{1-\lambda}\right\} y_{t}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
n_{t}^{H} & =(w_{t}-\gamma c_{t}^{H})/\varphi\\
 & =w_{t}/\varphi-\gamma(1+\tilde{\varphi})y_{t}/\varphi\\
 & =(\gamma c_{t}+\varphi n_{t})/\varphi-(\gamma/\varphi)(1+\tilde{\varphi})y_{t}\\
 & =(\gamma/\varphi)c_{t}+n_{t}-(\gamma/\varphi)(1+\tilde{\varphi})y_{t}\\
 & =\frac{1}{1-\alpha}y_{t}-(\gamma/\varphi)\tilde{\varphi}y_{t}\\
 & =\left(\frac{1}{1-\alpha}-\tilde{\gamma}\right)y_{t}\\
\tilde{\gamma} & =\gamma(1-\tau/\lambda)(1-\tilde{\Phi})
\end{align*}

