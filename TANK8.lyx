\begin_inset Formula 
\begin{align*}
c_{t}^{H} & =(1-\alpha)w_{t}+y_{t}+\frac{\tau^{D}}{\lambda}d_{t}\\
 & =\left(1+\frac{\tau^{D}}{\lambda}\Phi\right)y_{t}+(1-\alpha)\{1-(1-\Phi)\frac{\tau^{D}}{\lambda}\}w_{t}\\
 & =\left(1+\frac{\tau^{D}}{\lambda}\Phi\right)y_{t}+(1-\alpha)\{1-(1-\Phi)\frac{\tau^{D}}{\lambda}\}\{\gamma+\frac{\varphi}{1-\alpha}\}y_{t}\\
 & =\left(1+\frac{\tau^{D}}{\lambda}\Phi\right)y_{t}+\{1-(1-\Phi)\frac{\tau^{D}}{\lambda}\}\{(1-\alpha)\gamma+\varphi\}y_{t}\\
 & =\left(1+\frac{\tau^{D}}{\lambda}\Phi+\{1-(1-\Phi)\frac{\tau^{D}}{\lambda}\}\{(1-\alpha)\gamma+\varphi\}\right)y_{t}\\
\end{align*}

\end_inset


\begin_inset Formula 
